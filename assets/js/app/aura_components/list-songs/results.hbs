<table>
  <thead>
    <tr>
      <th>Artist</th>
      <th>Title</th>
      <th>Album</th>
    </tr>
  </thead>
  <tbody>
    {{#each songs.tracks}}
      <tr data-action-addSong="{{../party.id}}" data-songId="{{href}}">
        <td>{{artists.0.name}}</td>
        <td>{{name}}</td>
        <td>{{album.name}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
